<div class="my-0 px-0 pt-0" style="">
    <div class="row mx-0" style="">
        <div class="col-12 col-md-5 mb-3 px-2 px-lg-2 section-page-top-cont">
            <div class="row mx-0" style="">
                <div>
                {{{description}}}
                </div>
            </div>
            <div class="row mx-0" style="">
                {{#categorylist}}
                    {{#About}}
                        {{#subcategorycourses}}
                        <a class="stretched-link main-page-link-wrapper" href="{{itemurl}}"></a>
                            <div class="card border-0">
                                <img class="card-img w-100 d-block main-page-sub-card-img no-border-radius" src="{{itemimg}}">
                                <div class="card-img-overlay justify-content-start align-items-end d-flex flex-column">
                                    <h1 class="d-flex main-page-jumbo-title-cont main-page-link align-items-center w-100 justify-content-end">
                                        <span class="float-right main-page-jumbo-title d-flex flex-column card-text-over-picture p-2">
                                            <span class="main-page-link text-right">
                                                {{itemtitle}}
                                            </span>
                                            <span class="br text-right main-page-link main-page-sub-card-text">
                                                {{{itemdescription}}}
                                            </span>
                                            <span class="br text-right main-page-sub-card-text">
                                                <a class="main-page-link-sub text-white" href="itemtagurl">
                                                <small>
                                                    {{itemtag}}
                                                </small>
                                                </a>
                                            </span>
                                        </span>
                                        </h1>
                                </div>
                            </div>
                        {{/subcategorycourses}}
                    {{/About}}
                {{/categorylist}}
            </div>
        </div>
        <div class="col-12 col-md-7 mb-3 px-0 px-lg-2 section-page-top-cont">
            {{#categorylist}}
                    {{#Featured}}
                        {{#subcategorycourses}}
                        <h4 class="p-1 pt-2 m-0 main-page-section-title main-page-link w-100">{{Featured}}</h4>
                        <a class="stretched-link main-page-link-wrapper" href="{{itemurl}}"></a>
                            <div class="card border-0 pb-0 section-page-sub-card">
                                <img class="card-img w-100 d-block section-page-sub-card-img no-border-radius" src="{{itemimg}}">
                                <div class="card-img-overlay justify-content-start align-items-end d-flex flex-column">
                                    <h1 class="d-flex main-page-jumbo-title-cont main-page-link align-items-center w-100 justify-content-end">
                                        <span class="float-right main-page-jumbo-title d-flex flex-column card-text-over-picture p-2">
                                            <span class="main-page-link text-right">
                                                {{itemtitle}}
                                            </span>
                                            <span class="br text-right main-page-link main-page-sub-card-text">
                                                {{{itemdescription}}}
                                            </span>
                                            <span class="br text-right main-page-sub-card-text">
                                                <a class="main-page-link-sub text-white" href="{{{itemcaturl}}}">
                                                <small>
                                                    {{itemcat}}
                                                </small>
                                                </a>
                                            </span>
                                        </span>
                                        </h1>
                                </div>
                            </div>
                        {{/subcategorycourses}}
                    {{/Featured}}
                {{/categorylist}}
        </div>
    </div>
    <div class="row pt-3 mx-0 main-page-section-top-border">
        {{#categorylist}}
            {{^About}}
            {{^Featured}}
            {{^Committee}}
            {{^Elibrary}}
                <div class="col-md-4 col-lg-4 col-md-12 pb-3 px-0 px-lg-2">
                    <h4 class="text-center main-page-section-title pt-2 p-1 px-0 main-page-link">
                        <a class="section_title main-page-link" href="{{{categoryurl}}}">{{categorytitle}}</a>
                    </h4>
                    <ul class="list-group mx-0" style="border-style: none;">
                        {{#subcategorycourses}}
                                {{#first}}
                                    {{> theme_apoa/mainpage/subjumbo/subjumbolistitemtop}}
                                {{/first}}
                                {{^first}}
                                    {{> theme_apoa/mainpage/subjumbo/subjumbolistitem}}
                                {{/first}}
                        {{/subcategorycourses}}
                        <li class="list-group-item p-0 main-page-sub-list-group-item">
                            <div class="card p-0" style="border-style: none;">
                                <div class="card-body pb-0 pt-2">
                                    <p class="mb-1">
                                    <a class="main-page-link main-page-more-link" href="{{{categoryurl}}}">more {{categorytitle}}...</a>
                                    </p>
                                </div>
                            </div>
                        </li>
                    </ul>
                
                </div>
            {{/Elibrary}}
            {{/Committee}}
            {{/Featured}}
            {{/About}}
            {{#Elibrary}}
                
                <div class="col-md-4 col-lg-4 col-md-12 pb-3 px-0 px-lg-2">

                    <h4 class="text-center main-page-section-title pt-2 p-1 px-0 main-page-link">
                        <a class="section_title main-page-link" href="{{{categoryurl}}}">{{categorytitle}}</a>
                    </h4>
                    {{> theme_apoa/section/elibrarylist}}
                
                </div>
            {{/Elibrary}}
        {{/categorylist}}
    </div>
</div>
